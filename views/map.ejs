          <div class="col-sm-9">
            <input type="text" class="form-control" id="incident" name="incident" readOnly >
          </div>
        </div>
        <!-- User -->
        <div class="form-group row mx-sm-3 mb-2">
          <label for="email" class="col-sm-3 col-form-label">Email</label>
          <div class="col-sm-9">
            <input type="email" class="form-control" id="email" name="email" >
          </div>
        </div>
       
        <!-- Incident Date -->
        <div class="form-group row mx-sm-3 mb-2">
          <label for="date" class="col-sm-3 col-form-label" id="dt"><span>Date</span></label>
          <div class="col-sm-9">
            <input type="date" class="form-control" id="date" name="date" >
            <input type="number" class="form-control" id="number" name="number" >
          </div>
        </div>
        <!-- Incident Period -->
        <div class="form-group row mx-sm-3 mb-2">
          <label for="time" class="col-sm-3 col-form-label">Time</label>
          <div class="col-sm-9">
            <select id="time" name="time">
              <option value="morning">Morning</option>
              <option value="afternoon">Afternoon</option>
              <option value="evening">Evening</option>      
          </select>
            <!-- <input type="date" class="form-control" id="incident"disabled > -->
          </div>
        </div>
         <!-- Description -->
         <div class="form-group row mx-sm-3 mb-2">
          <label for="description" class="col-sm-3 col-form-label">Description</label>
          <div class="col-sm-9">
            <textarea id="description" name="description" placeholder="Write something.." style="height:200px "></textarea>
          </div>
        </div>

              
    <button type="submit" class="btn btn-success mb-3" id="grabPosition" >Report event</button>
  </form>




</ul>

      
  
